<!DOCTYPE html>
<html>
<head>
	<title><%= title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<% flash.each do |name, msg| %>
		<%= content_tag :div, msg, :id => "flash_#{name}" %>
	<% end %>

<%= yield %>

<%= debug(params) if Rails.env.development? %>

</body>
</html>
